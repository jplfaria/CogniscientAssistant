# Argo Proxy Configuration
# The proxy translates between Argo's API format and OpenAI's format
ARGO_PROXY_URL=http://localhost:8050/v1

# IMPORTANT: Never commit your actual username
# Set ARGO_USER in your local .env file (which should be in .gitignore)
ARGO_USER=

# Model Selection
# Available models: gpt4o, gpto3mini, claudeopus4, gemini25pro
DEFAULT_MODEL=gpt4o

# Optional: Override specific agent models
SUPERVISOR_MODEL=gpt4o
GENERATION_MODEL=claudeopus4
REFLECTION_MODEL=gpt4o
RANKING_MODEL=gpto3mini
EVOLUTION_MODEL=claudeopus4
PROXIMITY_MODEL=gpto3mini
META_REVIEW_MODEL=gpt4o

# Logging Configuration
LOG_LEVEL=INFO
VERBOSE_BAML=false

# Safety Configuration
ENABLE_SAFETY_CHECKS=true
SAFETY_MODEL=gpt4o

# Performance Configuration
MAX_CONCURRENT_AGENTS=5
AGENT_TIMEOUT_SECONDS=300